.c.rel:
	$(XCC) $(INCLUDES) $(DEFINES) \
		-c -o $@ `test -f '$<' || echo '$(srcdir)/'`$<

.a51.rel:
	test -f `basename '$<'` || $(LN_S) '$<' .
	test -f ../common/`basename '$<'` -o \
		\! -f `dirname '$<'`/../common/`basename '$<'` \
		|| $(LN_S) `dirname '$<'`/../common/`basename '$<'` ../common/`basename '$<'`
	$(XAS) `basename '$<'`

