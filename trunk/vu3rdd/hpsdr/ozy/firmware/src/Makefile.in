install-firmware1DATA: $(firmware1_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(firmware1dir)" || $(MKDIR_P) "$(DESTDIR)$(firmware1dir)"
	@list='$(firmware1_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f=$(am__strip_dir) \
	  echo " $(firmware1DATA_INSTALL) '$$d$$p' '$(DESTDIR)$(firmware1dir)/$$f'"; \
	  $(firmware1DATA_INSTALL) "$$d$$p" "$(DESTDIR)$(firmware1dir)/$$f"; \
	done

uninstall-firmware1DATA:
	@$(NORMAL_UNINSTALL)
	@list='$(firmware1_DATA)'; for p in $$list; do \
	  f=$(am__strip_dir) \
	  echo " rm -f '$(DESTDIR)$(firmware1dir)/$$f'"; \
	  rm -f "$(DESTDIR)$(firmware1dir)/$$f"; \
	done
