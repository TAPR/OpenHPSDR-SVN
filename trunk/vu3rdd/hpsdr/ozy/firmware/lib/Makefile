.c.rel:
	$(XCC) $(INCLUDES) $(DEFINES) -c $< -o $@

$(LIBRARY): $(LIBOBJS)
	-rm -f $(LIBRARY)
	touch $(LIBRARY)
	for obj in $(LIBOBJS); do basename $$obj .rel >> $(LIBRARY) ; done

