   0003 02                  101 	.db	>0x0200		; Specification version (MSB)
   0004 FF                  102 	.db	0xff		; device class (vendor specific)
   0005 FF                  103 	.db	0xff		; device subclass (vendor specific)
   0006 FF                  104 	.db	0xff		; device protocol (vendor specific)
   0007 40                  105 	.db	64		; bMaxPacketSize0 for endpoint 0
   0008 FE                  106 	.db	<VID_FREE	; idVendor
