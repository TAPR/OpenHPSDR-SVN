--synthesis_resources = reg 1 
OPTIONS ALTERA_INTERNAL_OPTION = "AUTO_SHIFT_REGISTER_RECOGNITION=OFF";

SUBDESIGN dffpipe_co8
( 
	clock	:	input;
	clrn	:	input;
	d[0..0]	:	input;
	q[0..0]	:	output;
) 
VARIABLE 
	dffe14a[0..0] : dffe;
	ena	: NODE;
	prn	: NODE;
	sclr	: NODE;

BEGIN 
	dffe14a[].CLK = clock;
	dffe14a[].CLRN = clrn;
	dffe14a[].D = (d[] & (! sclr));
	dffe14a[].ENA = ena;
	dffe14a[].PRN = prn;
	ena = VCC;
	prn = VCC;
	q[] = dffe14a[].Q;
	sclr = GND;
END;
