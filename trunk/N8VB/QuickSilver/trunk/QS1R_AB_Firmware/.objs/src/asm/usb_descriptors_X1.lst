   0000                      85 	.even	; descriptors must be 2-byte aligned for SUDPTR{H,L} to work
                             86 
                             87 	;; The .even directive isn't really honored by the linker.  Bummer!
                             88 	;; (There's no way to specify an alignment requirement for a given area,
                             89 	;; hence when they're concatenated together, even doesn't work.)
                             90 	;;
                             91 	;; We work around this by telling the linker to put USBDESCSEG
                             92 	;; at 0xE000 absolute.  This means that the maximimum length of this
                             93 	;; segment is 480 bytes, leaving room for the two hash slots
                             94 	;; at 0xE1EO to 0xE1FF.
                             95 	;;
                             96 
   0000                      97 _high_speed_device_descr::
   0000 12                   98 	.db	DSCR_DEVICE_LEN
   0001 01                   99 	.db	DSCR_DEVICE
   0002 00                  100 	.db	<0x0200		; Specification version (LSB)
   0003 02                  101 	.db	>0x0200		; Specification version (MSB)
   0004 FF                  102 	.db	0xff		; device class (vendor specific)
   0005 FF                  103 	.db	0xff		; device subclass (vendor specific)
   0006 FF                  104 	.db	0xff		; device protocol (vendor specific)
   0007 40                  105 	.db	64		; bMaxPacketSize0 for endpoint 0
   0008 FE                  106 	.db	<VID_FREE	; idVendor
   0009 FF                  107 	.db	>VID_FREE	; idVendor
