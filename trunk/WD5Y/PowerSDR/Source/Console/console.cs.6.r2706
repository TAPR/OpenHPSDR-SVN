					retval = SetupForm.PAGain10;
					break;
				case Band.B6M:
					if(current_model == Model.FLEX5000 || current_model == Model.FLEX3000)
						retval = SetupForm.PAGain10;
					else retval = 1000;
					break;
				default:
					retval = 1000;
					break;
			}

			return retval;
		}

