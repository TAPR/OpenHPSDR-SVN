		public string CATReadSWR()
		{
			double swr = 0.0;
			//pa_power_mutex.WaitOne();
			swr = SWR(pa_fwd_power, pa_rev_power);
			//pa_power_mutex.ReleaseMutex();
			return swr.ToString("f1")+" : 1";
		}

