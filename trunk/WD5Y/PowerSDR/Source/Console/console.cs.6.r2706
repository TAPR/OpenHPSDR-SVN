										{
											radio.GetDSPRX(0, 1).RXOsc = rx2_osc;
										}
										else chkEnableMultiRX.Checked = false;
									}
								}
							}
							break;
						case Model.SOFTROCK40:
#if true 
							//!!!!drm patch
							double osc_freq = soft_rock_center_freq*1e6 - freq*1e6;
