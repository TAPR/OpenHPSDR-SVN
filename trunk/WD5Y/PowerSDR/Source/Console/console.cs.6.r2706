		private void BroadcastFreqChange(string vfo, string freq)
		{
			freq = "F"+vfo+freq.Replace(separator,"").PadLeft(11,'0')+";";
			byte[] bfreq = ASCIIEncoding.ASCII.GetBytes(freq);
			try
			{
				siolisten.SIO.put(bfreq,(uint)bfreq.Length);
			}
			catch{}
		}

