			//int retval = 0;
			progress.SetPercent(0.0f);

			radio.GetDSPRX(1, 0).RXCorrectIQPhase = -400.0f;
			radio.GetDSPRX(1, 0).RXCorrectIQGain = -500.0f;

			float[] a = new float[Display.BUFFER_SIZE];

			Thread.Sleep(200);
			float sum = 0.0f;
			int peak_bin = -1;
			float max_signal = float.MinValue;

			for(int i=0; i<5; i++)
