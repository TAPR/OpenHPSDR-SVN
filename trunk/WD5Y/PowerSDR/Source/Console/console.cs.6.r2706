							((RadioButtonTS)c).Checked = false;

							if(c.BackColor != SystemColors.Control)
								((RadioButtonTS)c).BackColor = SystemColors.Control;
						}
					}
					return;
			}

			UpdateRX1Filters(low, high);
		}

