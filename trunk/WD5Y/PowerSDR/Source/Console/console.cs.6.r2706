			RX1DSPMode = DSPMode.SPEC;					// set dsp mode to DSB

			PreampMode preamp = rx1_preamp_mode;		// save current preamp setting
			RX1PreampMode = PreampMode.HIGH;			// set preamp to high

			bool duplex = full_duplex;
