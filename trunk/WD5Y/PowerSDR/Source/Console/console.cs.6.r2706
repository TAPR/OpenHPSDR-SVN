					grpModeSpecificPhone.BringToFront();
					break;
				case DSPMode.AM:
					radModeAM.BackColor = button_selected_color;
					grpMode.Text = "Mode - AM";
					if(!rx_only && chkPower.Checked)
						chkMOX.Enabled = true;
					if(chkVFOATX.Checked) 
					{
						chkMON.Checked = false;
						chkMON.Enabled = false;
