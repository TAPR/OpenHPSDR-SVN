					DateTime.Now.ToShortTimeString().Replace(":", ".");
				
				File.Copy(Application.StartupPath+"\\PowerSDR.mdb", desktop+"\\PowerSDR_"+datetime+".mdb");
				File.Delete(Application.StartupPath+"\\PowerSDR.mdb");
			}
		}

